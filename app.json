{
  "expo": {
    "name": "chompline",
    "slug": "chompline",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/chompline-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/chompline-adaptive-icon.png",
        "backgroundColor": "#2D6B4A"
      },
      "package": "com.abelkiprop.chompline",
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/chompline-favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/chompline-splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#2D6B4A"
        }
      ],
      "expo-web-browser",
      "expo-font",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1db55e28-9418-4d92-a355-8fb2767344a0"
      }
    }
  }
}
